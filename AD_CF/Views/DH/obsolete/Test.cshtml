                    <div class="row">
                        <div class="col-sm-12">
                            <div class="box">
                                <div class="box-head">
                                    <div class="container">
                                        @using (Html.BeginForm("CreateDelegation", "DH", FormMethod.Post))
                                        {<fieldset>
                                                <legend>Summary of Stationery Requisition (Pending Fulfilment)</legend>
                                                <div class="form-group">
                                                    <label for="dtp_input1" class="col-md-2 control-label">Date Picking</label>
                                                    <div class="input-group date form_date col-md-5" data-date="" data-date-format="yyyy-MM-dd" data-link-field="dtp_input1" data-link-format="yyyy-mm-dd">
                                                        <input class="form-control" size="16" type="text" value="" readonly>
                                                        <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
                                                        <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                                    </div>
                                                    @Html.HiddenFor(model => d.startDate, new { @id = "dtp_input1", @name = "startDate" })

                                                </div>
                                                <div class="form-group">
                                                    <label for="dtp_input2" class="col-md-2 control-label">Date Picking</label>
                                                    <div class="input-group date form_date col-md-5" data-date="" data-date-format="yyyy-MM-dd" data-link-field="dtp_input2" data-link-format="yyyy-mm-dd">
                                                        <input class="form-control" size="16" type="text" value="" readonly>
                                                        <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
                                                        <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                                    </div>
                                                    @Html.HiddenFor(model => d.endDate, new { @id = "dtp_input2", @name = "endDate" })
                                                </div>
                                            </fieldset>
                                            <input type="submit" value="Save" class="btn btn-default" />
                                        }
                                    </div>

                                </div><!--/.box-head-->
                            </div><!--/.box-->
                        </div><!--/.col-->
                    </div><!--/.row-->
                </div><!--/.container-fluid-->

            </div><!--/.main-content-->

        </div>
        <!-- END MAIN -->

        <div class="clearfix"></div>

        <footer>
            <div class="container-fluid">
                <p>while star</p>
            </div>

        </footer>

